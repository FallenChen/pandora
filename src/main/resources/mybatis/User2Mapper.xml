<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xulu.pandora.mapper.test2.User2Mapper">
    <resultMap id="BaseResultMap" type="com.xulu.pandora.entity.User2">
        <id column="id" property="id" jdbcType="Integer" />
        <result column="name" property="name" jdbcType="VARCHAR" />
        <result column="age" property="age" jdbcType="INTEGER" />
    </resultMap>

    <insert id="insert" parameterType="com.xulu.pandora.entity.User2" useGeneratedKeys="true" keyProperty="id">
        insert into user(name,age) values(#{name},#{age})
    </insert>
</mapper>